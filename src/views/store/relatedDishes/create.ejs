                </td>
                <td class="name-food">
                  <button type="button" class="js-modalOpen" data-target="modalsearch">
                    <span>Dish 4</span>
                  </button>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
        <a href="./edit-menu.html" class="btn-add-menu">
          <i class="fa-solid fa-plus"></i><span><%= __('store_page_related_dish_btn_add') %></span>
        </a>
      </div>
    </div>
  </section>
  <!-- popup main -->
  <div class="modal search-modal js-modalCnt" id="modalsearch">
    <div class="modal__wrap">
      <button type="button" type="button" class="modal__close js-modalClose">
        <i class="fa-solid fa-xmark"></i>
