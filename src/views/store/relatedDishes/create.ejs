    <div class="menu">
      <h2><%= __('store_page_related_dish_title_create') %></h2>
      <div class="box-menu">
        <div class="box-item">
          <label for=""><%= __('store_page_related_dish_lb_group_name') %></label>
          <div class="custom-arrow">
            <input type="text" />
          </div>
        </div>
      </div>
    </div>
    <div class="menu-detail">
      <div class="box-menu-detail">
        <div class="table-food">
          <table>
            <thead>
              <th class="img-food"><%= __('store_page_related_dish_lb_image') %></th>
              <th class="name-food"><%= __('store_page_related_dish_lb_name') %></th>
            </thead>
            <tbody>
              <tr>
                <td class="img-food">
                  <img src="<%= appUrl %>/stores/img/food/demo-food.jpg" alt="" />
                </td>
                <td class="name-food">
                  <button type="button" class="js-modalOpen" data-target="modalsearch">
                    <span>Dish 1</span>
                  </button>
                </td>
              </tr>
              <tr>
                <td class="img-food">
                  <img src="<%= appUrl %>/stores/img/food/demo-food.jpg" alt="" />
                </td>
                <td class="name-food">
                  <button type="button" class="js-modalOpen" data-target="modalsearch">
                    <span>Dish 2</span>
                  </button>
